    "react-dom": "^0.14.0",
    "node-static": "^0.7.7"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.10"
  },
  "browser": {
    "react-intl": "../../lib/react-intl.js"
