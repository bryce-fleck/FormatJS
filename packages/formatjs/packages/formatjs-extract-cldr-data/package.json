{
  "name": "formatjs-extract-cldr-data",
  "version": "9.4.0",
  "description": "Utility library that extracts the CLDR data that's needed by the FormatJS libraries.",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "jest",
    "mjs": "find ./dist -depth -name \"*.js\" -exec sh -c 'f=\"{}\"; mv -- \"$f\" \"${f%.js}.mjs\"' \\;",
    "cldr": "TS_NODE_PROJECT='scripts/tsconfig.json' ts-node scripts/aggregateLocaleFiles",
    "build": "npm run cldr && tsc && npm run mjs && tsc -p tsconfig.cjs.json",
    "cover": "npm t -- --coverage"
  },
  "author": "Eric Ferraiuolo <edf@ericf.me>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@types/glob": "^7.1.1",
    "cldr-core": "^35.0.0",
    "cldr-dates-full": "^35.0.0",
    "glob": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/formatjs/formatjs.git"
  },
  "keywords": [
    "i18n",
    "cldr",
    "formatjs"
  ],
  "bugs": {
    "url": "https://github.com/formatjs/formatjs/issues"
  },
  "homepage": "https://github.com/formatjs/formatjs",
  "gitHead": "8b0baec8eda5002715cf893274fe59782fc2d371"
}
